{
    "name": "blog-api-doc",
    "version": "1.0.0",
    "description": "my blog api doc",
    "keywords": [
        "apidoc"
    ],
    "author": "yemiancheng <ymc.github@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev-doc": "nodemon --watch src/apidoc.js --exec \"node src/apidoc.js && npm run docs:be-api\"",
        "lint": "eslint --ext .js src/ --fix",
        "docs:be-api": "apidoc -i src/ -o docs/",
        "docs:src": "cross-env NODE_ENV=production jsdoc --configure .jsdoc.json.js"
    },
    "devDependencies": {
        "apidoc": "^0.17.6",
        "cross-env": "^5.2.0",
        "eslint-config-lcy-vue": "^2.2.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.2",
        "jsdoc": "^3.5.5",
        "nodemon": "^1.18.4",
        "prettier": "^1.13.7",
        "tui-jsdoc-template": "^1.2.2"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 5.0.0"
    }
}
